    "@hookform/resolvers": "^2.9.1",
    "@reduxjs/toolkit": "^1.8.2",
    "classnames": "^2.3.1",
    "firebase": "^9.8.3",
    "normalize.css": "^8.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.32.0",
    "react-hot-toast": "^2.2.0",
    "react-redux": "^8.0.2",
